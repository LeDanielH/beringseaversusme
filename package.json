{
	"name": "gatsby-starter-hello-world",
	"private": true,
	"description": "A simplified bare-bones starter for Gatsby",
	"version": "0.1.0",
	"license": "0BSD",
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"start": "npm run gatsby-config && npm run develop",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
		"gatsby-config": "tsc gatsby-node.ts --skipLibCheck true --noImplicitUseStrict true --target es5 --jsx preserve"
	},
	"dependencies": {
		"@householdjs/elements": "^2.2.0-alpha.0",
		"@householdjs/types": "^2.2.0-alpha.0",
		"@householdjs/ui": "^2.2.0-alpha.0",
		"@householdjs/utils": "^2.2.0-alpha.0",
		"@mdx-js/mdx": "^1.6.16",
		"@mdx-js/react": "^1.6.16",
		"gatsby": "^2.24.52",
		"gatsby-image": "^2.4.16",
		"gatsby-plugin-google-analytics": "^2.3.13",
		"gatsby-plugin-graphql-codegen": "^2.7.1",
		"gatsby-plugin-intl": "^0.3.3",
		"gatsby-plugin-manifest": "^2.4.23",
		"gatsby-plugin-mdx": "^1.2.39",
		"gatsby-plugin-offline": "^3.2.23",
