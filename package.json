{
	"name": "gatsby-starter-hello-world",
	"private": true,
	"description": "A simplified bare-bones starter for Gatsby",
	"version": "0.1.0",
	"license": "0BSD",
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"start": "npm run gatsby-config && npm run develop",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"check:type": "tsc --noEmit",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
		"gatsby-config": "tsc gatsby-node.ts --skipLibCheck true --noImplicitUseStrict true --target es5 --jsx preserve"
	},
	"dependencies": {
		"@householdjs/elements": "^2.2.0-alpha.0",
		"@householdjs/types": "^2.2.0-alpha.0",
		"@householdjs/ui": "^2.2.0-alpha.0",
		"@householdjs/utils": "^2.2.0-alpha.0",
		"@mdx-js/mdx": "^1.6.22",
		"@mdx-js/react": "^1.6.22",
		"gatsby": "^4.8.1",
		"gatsby-plugin-google-analytics": "^4.8.0",
		"gatsby-plugin-graphql-codegen": "^3.1.1",
		"gatsby-plugin-image": "^2.8.1",
		"gatsby-plugin-manifest": "^4.8.1",
		"gatsby-plugin-mdx": "^3.15.2",
		"gatsby-plugin-offline": "^5.8.1",
		"gatsby-plugin-react-helmet": "^5.8.0",
		"gatsby-plugin-react-intl": "^3.0.2",
		"gatsby-plugin-sharp": "^4.8.1",
		"gatsby-plugin-styled-components": "^5.8.0",
		"gatsby-remark-component": "^1.1.3",
		"gatsby-remark-copy-linked-files": "^5.8.0",
		"gatsby-source-filesystem": "^4.8.1",
		"gatsby-transformer-sharp": "^4.8.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet": "^6.1.0",
		"styled-components": "^5.3.3",
		"ts-optchain": "^0.1.8",
		"typescript": "^4.5.5",
		"utility-types": "^3.10.0"
	},
	"devDependencies": {
		"@types/node": "^17.0.21",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.11",
		"@types/react-helmet": "^6.1.5",
		"@types/rehype-react": "^4.0.0",
		"@types/styled-components": "^5.1.23",
		"babel-plugin-styled-components": "^2.0.6",
		"csstype": "^3.0.10",
		"dotenv": "^16.0.0",
		"prettier": "2.5.1",
		"react-apollo": "2.5.8"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	}
}
